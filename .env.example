# Azure Speech Service (Required)
SPEECH_REGION=westus2
SPEECH_KEY=your_speech_key_here

# Azure Speech Service Private Endpoint (Optional)
SPEECH_PRIVATE_ENDPOINT=https://my-speech-service.cognitiveservices.azure.com/
SPEECH_RESOURCE_URL=/subscriptions/your-subscription-id/resourceGroups/my-rg/providers/Microsoft.CognitiveServices/accounts/my-speech
USER_ASSIGNED_MANAGED_IDENTITY_CLIENT_ID=your_client_id_here

# Azure OpenAI (Required for chat scenario)
AZURE_OPENAI_ENDPOINT=https://my-aoai.openai.azure.com/
AZURE_OPENAI_API_KEY=your_openai_api_key_here
AZURE_OPENAI_DEPLOYMENT_NAME=my-gpt-35-turbo-deployment

# Azure Cognitive Search (Optional, for 'on your data' scenario)
COGNITIVE_SEARCH_ENDPOINT=https://my-cognitive-search.search.windows.net/
COGNITIVE_SEARCH_API_KEY=your_cognitive_search_api_key_here
COGNITIVE_SEARCH_INDEX_NAME=my-search-index

# Custom ICE Server (Optional)
ICE_SERVER_URL=turn:x.x.x.x:3478
ICE_SERVER_URL_REMOTE=turn:x.x.x.x:3478
ICE_SERVER_USERNAME=your_ice_username
ICE_SERVER_PASSWORD=your_ice_password
